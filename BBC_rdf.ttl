####################### Building triples for BBC Outlet #####################

#Event 1
ex:E_BBC_1                  a                       ex:Event  {| ex:occuredatTime "2021-01-06T08:17:00"^^xsd:datetime|}.
ex:StamentTrump_BBC_1       a                       ex:Statment;
                            rdfs:label              <https://ichef.bbci.co.uk/news/976/cpsprodpb/13E18/production/_116923418_21e80ad3-11f0-4fcf-a3f4-42c19ce312ab.jpg.webp>.
ex:donaldTrump              ex:gaveStatement        ex:StamentTrump_BBC_1 {| ex:occuredatTime "2021-01-06T08:17:00"^^xsd:datetime|}.                     
BBC:bbc                     a                       ex:News_Outlet;
                            ex:Describes            ex:E_BBC_1.



#Event 2
ex:towardsTheCapitol        a                      foaf:Location.
ex:E_BBC_2                  a                      ex:Event {| ex:occuredatTime "2021-01-06T09:10:00"^^xsd:datetime|};
                            ex:occuredOn           ex:towardsTheCapitol {| ex:occuredatTime "2021-01-06T09:10:58"^^xsd:datetime|}.
ex:statementDG_BBC_1        a                      ex:Statment;
                            rdfs:label             "We're taking our country back.".              
ex:davidGroseman            ex:memberOf            ex:proudBoys;
                            ex:gaveStatement       ex:statementDG_BBC_1 {| ex:occuredatTime "2021-01-06T09:10:58"^^xsd:datetime|}.  
ex:statementCBBC_BBC_1      a                      ex:Statement;
                            rdfs:label             "It was clear he was communicating - getting messages, sending messages to somebody" .
ex:correspondentBBC         ex:gaveStatement       ex:statementCBBC_BBC_1 {| ex:occuredatTime "2021-01-06T09:10:58"^^xsd:datetime|}.
BBC:bbc                     ex:Describes           ex:E_BBC_2.


#Event 3
ex:E_BBC_3                  a ex:Event             {| ex:occuredatTime "2021-01-06T12:53:00"^^xsd:datetime|}.
ex:StamentTrump_BBC_2       a                      ex:Statment;
                            rdfs:label             "I know that everyone here will soon be marching over to the Capitol building to peacefully and patriotically make your voices heard".
ex:donaldTrump              ex:gaveStatement       ex:StamentTrump_BBC_2 {| ex:occuredatTime "2021-01-06T12:00:00"^^xsd:datetime|}.
BBC:bbc                     ex:Describes           ex:E_BBC_3. 

#Event 4
ex:outsideTheCapitol        a                      foaf:Location.
ex:E_BBC_4                  a                      ex:Event {| ex:occuredatTime "2021-01-06T12:53:00"^^xsd:datetime|};
                            ex:occuredOn           ex:outsideTheCapitol {| ex:occuredatTime "2021-01-06T12:53:00"^^xsd:datetime|}.
ex:crowd_statement_BBC_1    a                      ex:Statement;
                            rdfs:label             "Fight for Trump.".                         
ex:crowdOutsideTheCapitol   ex:attacked            ex:police {| ex:occuredatTime "2021-01-06T12:53:00"^^xsd:datetime|};
                            ex:gaveStatement       ex:crowd_statement_BBC_1 {| ex:occuredatTime "2021-01-06T12:53:00"^^xsd:datetime|}.





