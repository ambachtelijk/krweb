####################### Building triples for FOX Outlet #####################

# Event 1
ex:e_FOX_1                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T10:02:00"^^xsd:datetime|}.
ex:StatementPence_FOX_1         a                   ex:Statement;         
                                rdfs:label          "It is my considered judgment that my oath to support and defend the Constitution constrains me from claiming unilateral authority to determine which electoral votes should be counted and which should not.".
ex:mikePence                    ex:gaveStatement    ex:e_FOX_1          {| ex:occuredatTime "2021-01-06T10:02:00"^^xsd:datetime|}.
FOX:fox29                       a                   ex:News_Outlet;
                                ex:Describes        ex:e_FOX_1.

# Event 2
ex:e_FOX_2                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T11:00:00"^^xsd:datetime|}.
ex:StatementTrump_FOX_1         a                   ex:Statement;
                                rdfs:label          "We will not let them silence your voices, we will stop the steal".
ex:donaldTrump                  ex:gaveStatement    ex:e_FOX_2.
FOX:fox29                       ex:Describes        ex:e_FOX_2.

# Event 3
ex:e_FOX_3                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T13:10:00"^^xsd:datetime|}.
ex:lawmakers                    ex:communicatedWith ex:lawmakers;
                                rdfs:label          "debate on the counting of the votes in Arizona".
FOX:fox29                       ex:Describes        ex:e_FOX_3.

# Event 4
ex:e_FOX_4                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T13:47:00"^^xsd:datetime|}.   
ex:protestors                   ex:attacked         ex:police.
ex:capitolPolice                ex:evacuates        ex:congress.
FOX:fox29                       ex:Describes        ex:e_FOX_4.

# Event 5
ex:e_FOX_5                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T14:00:00"^^xsd:datetime|}.
ex:StatementFOX_reporter_1      a                   ex:Statement;
                                rdfs:label          "rioters had gotten into the U.S. Capitol building".
FOX:FOXreporter                 ex:gaveStatement    ex:StatementFOX_reporter_1.
ex:police                       ex:attacked         ex:rioters.
FOX:fox29                       ex:Describes        ex:e_FOX_5.

# Event 6
ex:e_FOX_6                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T14:20:00"^^xsd:datetime|}.
ex:capitolPolice                ex:communicatedWith ex:lawmakers;
                                rdfs:label          "Due to an "external security threat," no one could enter or exit the Capitol complex.".
FOX:fox29                       ex:Describes        ex:e_FOX_6.

# Event 7
ex:e_FOX_7                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T14:40:00"^^xsd:datetime|}.
ex:CapitolRotunda               a                   foaf:Location
ex:rioters                      ex:attacked         ex:police.
ex:police                       ex:attacked         ex:rioters.
FOX:fox29                       ex:Describes        ex:e_FOX_7.

# Event 8
ex:e_FOX_8                      a                   ex:Event           {| ex:occuredatTime "2021-01-06T15:30:00"^^xsd:datetime|}.
ex:capitolPolice                ex:attacked         ex:attendees;
                                rdfs:label          "One person was shot. The condition of the person is unclear.".
FOX:fox29                       ex:Describes        ex:e_FOX_8.

# Event 9
ex:e_FOX_9                      a                   ex:Event            {| ex:occuredatTime "2021-01-06T15:36:00"^^xsd:datetime|}.
ex:StatementchrisMiller_FOX_1   a                   ex:Statement;
                                rdfs:label          "We have fully activated the D.C. National Guard to assist federal and local law enforcement as they work to peacefully address the situation. We are prepared to provide additional support as necessary and appropriate as requested by local authorities. Our people are sworn to defend the constitution and our democratic form of government and they will act accordingly.".
ex:chrisMiller                  ex:gaveStatement    StatementchrisMiller_FOX_1.
FOX:fox29                       ex:Describes        ex:e_FOX_9.                        

# Event 10
